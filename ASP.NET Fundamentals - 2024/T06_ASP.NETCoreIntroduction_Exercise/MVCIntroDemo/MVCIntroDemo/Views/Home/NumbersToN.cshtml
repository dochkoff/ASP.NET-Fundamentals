@{
    ViewBag.Title = "Numbers 1..." + ViewBag.Count;
}

<h2>@ViewBag.Title</h2>

<label for="countInput">Set Nunmber</label>
<input type="number" id="countInput" value="@ViewBag.Count"/>
<button class="btn btn-primary" onclick="SetLimit()">Submit</button>

<ul>
    @for (int i = 1; i <= ViewBag.Count; i++)
    {
        <li>@i</li>
    }
</ul>

@section Scripts {
    <script src="~/js/numbers.js"></script>
}